package tasks

import (
    "templ/models"
    )

templ Table(tks models.Tasks){
    <div id="list" class="flex flex-col rounded-lg w-full bg-slate-800 divide-y divide-gray-600 mt-5" hx-get="/" hx-trigger="updateList" hx-swap="outerHTML" >
        for index, item := range tks {
            @Item(item, index)
        }
    </div>
}